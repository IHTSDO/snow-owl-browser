<br/>
<h4>
	<span ondrop="dropC(event, '{{divElementId}}')" ondragleave="removeHighlight();" ondragover="allowDrop(event)">{{id}} |{{fsn}}|</span>
</h4>
<h4>
<div class="btn-group" style="margin:20px" role="group" aria-label="...">
	<button type="button" class="btn btn-default" id="ads-{{divElementId}}-siblings-button">Siblings</button>
	<button type="button" class="btn btn-default" id="ads-{{divElementId}}-descendants-button">Descendants</button>
</div>
<div class="btn-group" style="margin:20px" role="group" aria-label="...">
	<button type="button" class="btn btn-default" id="ads-{{divElementId}}-Prim-button">Primitive</button>
	<button type="button" class="btn btn-default" id="ads-{{divElementId}}-FD-button">FullyDefined</button>
	<button type="button" class="btn btn-default" id="ads-{{divElementId}}-Both-button">Both</button>
</div>
{{ adsView }}s: {{ count }} using {{ selectedTemplates.length }} templates</h4>
</h4>
{{#each selectedTemplates}}
	<hr/>
	<div class="pull-right">
		<div class="pull-left">
			<h4>Usage: {{count}}  Examples: &nbsp;</h4>
			<ul>
			{{#each examples }}
				<li><a href="" title="{{ fsn }} ">{{ id }}</a></li>
			{{/each}}
			</ul>
		</div>
	</div>
	<div id="ads-{{@index}}" style="margin-top: 10px;" class="panel panel-default">
		<div id="ads-{{@index}}-diagram-body" class="hasSVG">
		</div>
	</div>
{{/each}}